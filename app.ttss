/*主题配色*/
page {
    --primary-bg-color: #fff;
    --secondary-bg-color: #f0f0f0;
    --primary-text-color: #333;
    --secondary-text-color: #666;
    --tertiary-text-color: #999;
    --icons: url(data:image/png;base64,);
    font-size: 16px;
    line-height: 1.2;
    color: var(--primary-text-color);
    background: var(--secondary-bg-color);
}
/*
@media(prefers-color-scheme: dark){
    --primary-bg-color: #1a1a1a;
    --secondary-bg-color: #000;
    --primary-text-color: #ccc;
    --secondary-text-color: #a0a0a0;
    --tertiary-text-color: #7b7b7b;
}
*/

image, video {
    max-width: 100%;
}
rich-text {
    user-select: text;
    -webkit-user-select: text;
}
.primary-title {
    font-size: 22px;
    font-weight: 600;
    color: var(--primary-text-color);
    user-select: text;
    -webkit-user-select: text;
}
.secondary-title {
    font-size: 20px;
    font-weight: 600;
    color: var(--primary-text-color);
}
.tertiary-title {
    font-size: 18px;
    color: var(--primary-text-color);
}
.section {
    padding: 10px;
    background: var(--primary-bg-color);
}
.entry-content {
    display: block;
    margin-top: 15px;
    font-size: 1em;
    line-height: 1.6;
    color: var(--secondary-text-color);
}

.entry-content .node-img, .entry-content .node-video {
    max-width: 100%;
    height: auto !important;
    max-height: 100% !important;
    vertical-align: bottom;
}
.entry-content .node-p, .entry-content .node-div {
    margin: 10px 0;
}
.entry-content .node-strong, .entry-content .node-h2, .entry-content .node-h3, .entry-content .node-h4, .entry-content .node-h5 {
    color: var(--primary-text-color);
}
.entry-content .node-h2 {
    font-size: 20px;
    margin: 16px 0 12px 0;
}
.entry-content .node-h3 {
    font-size: 18px;
    margin: 14px 0 12px 0;
}
.entry-content .node-h4 {
    font-size: 16px;
    margin: 12px 0 10px 0;
}
.entry-content .node-h5, .entry-content .node-h6 {
    font-size: 16px;
    margin: 10px 0;
}
.entry-content .node-ul {
    margin: 8px 0 0 0;
    line-height: 1.5;
    list-style: outside disc;
}
.entry-content .node-ol {
    margin: 8px 0 0 0;
    line-height: 1.5;
    list-style: outside decimal;
}
.entry-content .node-li {
    margin: 2px 0;
}
.entry-content .node-li::marker {
    margin-right: 2px;
}
.entry-content .node-dl .node-dt {
    margin: 10px 0;
    font-weight: 600;
}
.entry-content .node-dl .node-dd {
    margin: 8px 0 0 8px;
}
.entry-content .node-table {
    margin: 10px auto;
    border-collapse: collapse;
}
.entry-content .node-table .node-td {
    padding: 5px;
    border: 1px solid var(--primary-text-color);
    word-break: break-all;
}
.entry-content .node-table .node-tr:nth-child(2n+1) {
    background: #eee;
}
.entry-content .node-navigation {
    color: #549ccf;
}
.loading {
    padding: 15px 0;
    text-align: center;
    background: var(--primary-bg-color);
}
.loading text {
    display: inline-block;
    width: 6px;
    height: 6px;
    border-radius: 50%;
    margin: 0 3px;
    background: #bae212;
    animation-name: loading;
    animation-duration: .8s;
    animation-iteration-count: infinite;
}
.loading text:nth-child(1) {
    animation-delay: 0s;
}
.loading text:nth-child(2) {
    animation-delay: .15s;
}
.loading text:nth-child(3) {
    animation-delay: .3s;
}
.loading text:nth-child(4) {
    animation-delay: .45s;
}
.loading text:nth-child(5) {
    animation-delay: .6s;
}
@-webkit-keyframes loading {
    0%{
        transform: scale(1);
    }
    20%{
        transform: scale(1.1);
    }
    40%{
        transform: scale(1.2);
    }
    60%{
        transform: scale(1.3);
    }
    80%{
        transform: scale(1.4);
    }
    100%{
        transform: scale(1);
    }
}

